#totop
  position fixed
  display none
  z-index 9999
  bottom 5em
  right 1em
  cursor pointer
  @media tabletmax
    display none !important

#back-to-top {
    position: fixed;
    width: 48px;
    height: 48px;
    right: 40px;
    bottom: 60px;
    background: white;
    background: rgba(255, 255, 255, 0.3);
    border-radius: 24px;
    box-shadow: 0px 1px 8px 1.5px rgba(0, 0, 0, 0.35), 0px 20px 70px 8px rgba(0, 0, 0, 0.25);
    cursor: pointer;
    -webkit-user-select: none;
    -webkit-touch-callout: none;
}


#rocket
    margin: 8px auto
    display: block